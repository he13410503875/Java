CREATE TABLE `app_customer_indentity_info` (
  `customerId` varchar(38) CHARACTER SET utf8 NOT NULL COMMENT '客户编号',
  `customerName` varchar(20) CHARACTER SET utf8 DEFAULT NULL COMMENT '客户名称',
  `sex` varchar(5) CHARACTER SET utf8 DEFAULT NULL COMMENT '性别',
  `birthday` varchar(10) CHARACTER SET utf8 DEFAULT NULL COMMENT '出生日期',
  `certType` varchar(10) CHARACTER SET utf8 DEFAULT NULL COMMENT '证件类别',
  `certId` varchar(20) CHARACTER SET utf8 DEFAULT NULL COMMENT '证件号码',
  `maritalStatus` varchar(20) CHARACTER SET utf8 DEFAULT NULL COMMENT '婚姻状况',
  `familyAddress` varchar(200) CHARACTER SET utf8 DEFAULT NULL COMMENT '家庭住址',
  `mobile` varchar(12) CHARACTER SET utf8 DEFAULT NULL COMMENT '手机号码',
  `unitNature` varchar(20) CHARACTER SET utf8 DEFAULT NULL COMMENT '单位性质',
  `companyName` varchar(60) CHARACTER SET utf8 DEFAULT NULL COMMENT '单位/公司名称',
  `phoneNumber` varchar(12) CHARACTER SET utf8 DEFAULT NULL COMMENT '其它手机号码',
  `educationLevel` varchar(10) CHARACTER SET utf8 DEFAULT NULL COMMENT '文化程度',
  `monthlyIncome` decimal(10,0) DEFAULT NULL COMMENT '家庭月收入',
  `houseAddrees` varchar(200) CHARACTER SET utf8 DEFAULT NULL COMMENT '家庭住址',
  `houseType` varchar(20) CHARACTER SET utf8 DEFAULT NULL COMMENT '房产类别',
  `creditMaxMoney` varchar(25) CHARACTER SET utf8 DEFAULT NULL COMMENT '单张信用卡最高额度',
  `inputIP` varchar(15) CHARACTER SET utf8 DEFAULT NULL COMMENT '数据录入IP地址',
  `inputTime` datetime DEFAULT CURRENT_TIMESTAMP COMMENT '录入时间',
  `updateIP` varchar(15) CHARACTER SET utf8 DEFAULT NULL COMMENT '数据更改IP地址',
  `updateTime` datetime DEFAULT CURRENT_TIMESTAMP COMMENT '更改时间',
  `customerType` varchar(15) CHARACTER SET utf8 DEFAULT NULL COMMENT '客户类别',
  `recommendCode` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '推荐码',
  `recommendUserId` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '推荐用户ID',
  `idCardBeginDate` varchar(20) CHARACTER SET utf8 DEFAULT NULL COMMENT '身份证起始日',
  `idCardEndDate` varchar(20) CHARACTER SET utf8 DEFAULT NULL COMMENT '身份证到期日',
  PRIMARY KEY (`customerId`),
  KEY `customerId_index` (`customerId`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='借款客户身份信息';


CREATE TABLE `app_acct_loan` (
  `serialNo` varchar(40) CHARACTER SET utf8 NOT NULL COMMENT '流水号',
  `putoutNo` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '提现流水号',
  `accountNo` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '清算流水号',
  `coustomerId` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '客户编号',
  `coustomerName` varchar(20) CHARACTER SET utf8 DEFAULT NULL COMMENT '客户名称',
  `currency` tinyint(5) DEFAULT '1' COMMENT '货币编号 1-人民币 ',
  `businessSum` decimal(20,2) DEFAULT '0.00' COMMENT '剩余本金',
  `originalBusinessSum` decimal(20,2) DEFAULT '0.00' COMMENT '原始交易金额',
  `prepayments` decimal(10,2) DEFAULT '0.00' COMMENT '预付款',
  `putoutDate` datetime DEFAULT NULL COMMENT '提现日期',
  `maturityDate` datetime DEFAULT NULL COMMENT '到期日期',
  `lastDueDate` date DEFAULT NULL COMMENT '最近一次付款日期',
  `lastDuePayType` tinyint(2) DEFAULT NULL COMMENT '最近一次还款类型  1-还款日还款；2-逾期还款；3-提前还款；4-提前结清还款',
  `nextDueDate` date DEFAULT NULL COMMENT '下一期应付款日期',
  
  `loanStatus` tinyint(2) DEFAULT '1' COMMENT '放款状态  1-待放款；2-放款中；3-放款失败；4-放款成功',
  `repayStatus` tinyint(2) DEFAULT NULL COMMENT '还款状态 1-未还清；2-已经还清',
  `currentPeriod` tinyint(2) DEFAULT NULL COMMENT '当前期数',
  `totalPeriods` tinyint(2) DEFAULT NULL COMMENT '总期数 （一次性：1；分期：期数）',
  `settleDate` date DEFAULT NULL COMMENT '开始处理日期',
  `finishDate` date DEFAULT NULL COMMENT '处理完成时间',
  `autoPayFlag` int(1) DEFAULT '1' COMMENT '自动支付标识 1-自定',
  `finalInstalmentFlag` varchar(5) CHARACTER SET utf8 DEFAULT NULL COMMENT '最后分期付款标识',
  `businessDate` datetime DEFAULT NULL COMMENT '交易日期',
  `operateOrgId` varchar(10) CHARACTER SET utf8 DEFAULT NULL COMMENT '操作机构编号',
  `interestRate` decimal(24,20) DEFAULT NULL COMMENT '利息利率',
  `serviceRate` decimal(10,6) DEFAULT NULL COMMENT '服务费利率',
  `lateFeeRate` decimal(10,6) DEFAULT NULL COMMENT '滞纳金利率',
  `breachFeeRate` decimal(10,6) DEFAULT NULL COMMENT '违约金利率',
  `commission` decimal(10,2) DEFAULT NULL COMMENT '手续费',
  `relativeProjectNo` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '签约版本号',
  `ruleNo` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '获取签约版本使用的规则流水号',
  `cvuiSerialNo` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '签约版本使用详情流水号',
  `capitalSubjectNo` varchar(10) CHARACTER SET utf8 DEFAULT NULL COMMENT '资金方科目号',
  `accountingOrgId` varchar(10) CHARACTER SET utf8 DEFAULT NULL COMMENT '账单机构编号,A:小额 B:外贸信托 C:小额合并',
  `productType` varchar(10) CHARACTER SET utf8 DEFAULT NULL COMMENT '交易产品类型, 1 - 一次性还款; 2-分期还款',
  `businessMode` varchar(10) CHARACTER SET utf8 DEFAULT NULL COMMENT '交易方式 A:自由资金 B: 信托资金 C:银行资金',
  `tradeId` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT 'APP提现编号',
  `applyId` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT 'APP额度申请单号',
  `capFinishDate` date DEFAULT NULL COMMENT '资金方完成日期',
  `createTime` datetime DEFAULT NULL COMMENT '创建时间',
  `updateTime` datetime DEFAULT NULL COMMENT '更新时间',
  `paymentSerialNo` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '报盘流水号',
  `loanProduct` varchar(10) CHARACTER SET utf8 DEFAULT NULL COMMENT '产品类型',
  `clearDate` datetime DEFAULT NULL COMMENT '结清日期',
  `isWithhold` tinyint(2) DEFAULT '1' COMMENT '是否代扣 1-是; 2-否',
  `creditRating` varchar(20) CHARACTER SET utf8 DEFAULT NULL COMMENT '信用级别',
  `loanProductId` varchar(100) CHARACTER SET utf8 DEFAULT NULL COMMENT '产品ID',
  `channelNo` varchar(80) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT '渠道编号',
  `channelName` varchar(80) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT '渠道名称',
  `externalStatus` tinyint(2) DEFAULT '1' COMMENT '第三方状态 1提交第三方 2第三方返回成功(汇信成功也是2) 3第三方返回失败',
  `guaranteeSummaryNo` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '质保金入账表流水号(对应表 cts_guarantee_money_summary)',
  `guaranteeRate` decimal(10,6) DEFAULT NULL COMMENT '质保金比例',
  `guaranteeMoney` decimal(20,2) DEFAULT NULL COMMENT '质保金金额',
  `loanMoney` decimal(20,2) DEFAULT NULL COMMENT '实际放款金额',
  `guaranteeAccountId` varchar(40) CHARACTER SET utf8 DEFAULT NULL COMMENT '质保金账户ID',
  `guaranteeTransferWay` tinyint(2) DEFAULT '0' COMMENT '质保金代付 0-手动批量代付; 1-自动单个报盘代付',
  `interestRateUnit` tinyint(2) DEFAULT '2' COMMENT '利率单位 1-日利率, 2-月利率, 3-年利率',
  `clearFlag` tinyint(2) DEFAULT '1' COMMENT '每期还清标识 1-按应还金额, 2-按期款',
  `overdueCalType` tinyint(2) DEFAULT '2' COMMENT '逾期计算类型 1-不计算逾期, 2-按还款日计算, 3-还款日+1',
  `contractMoney` decimal(20,2) DEFAULT NULL COMMENT '合同金额',
  `riskRate` decimal(10,6) DEFAULT '0.000000' COMMENT '风险金比例（三费一金）',
  `riskMoney` decimal(20,2) DEFAULT '0.00' COMMENT '风险金金额（三费一金）',
  `consultationRate` decimal(10,6) DEFAULT '0.000000' COMMENT '咨询费比例（三费一金）',
  `consultationMoney` decimal(20,2) DEFAULT '0.00' COMMENT '咨询费金额（三费一金）',
  `evaluationRate` decimal(10,6) DEFAULT '0.000000' COMMENT '评估费比例（三费一金）',
  `evaluationMoney` decimal(20,2) DEFAULT '0.00' COMMENT '评估费金额（三费一金）',
  `managementRate` decimal(10,6) DEFAULT '0.000000' COMMENT '管理费比例（三费一金）',
  `managementMoney` decimal(20,2) DEFAULT '0.00' COMMENT '管理费金额（三费一金）',
  `riskFactor` varchar(20) COLLATE utf8_unicode_ci DEFAULT '1' COMMENT '用户风险系数',
  `commisionMoney` decimal(20,2) DEFAULT '0.00' COMMENT '平台手续费-四费',
  `commisionRate` decimal(10,6) DEFAULT '0.000000' COMMENT '平台手续费费率-四费',
  PRIMARY KEY (`serialNo`),
  KEY `tradeId_index` (`tradeId`) USING BTREE,
  KEY `customerId_index` (`coustomerId`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci COMMENT='放款信息表';


